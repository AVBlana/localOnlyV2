datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Experience {
  id          String   @id @default(cuid())
  title       String
  location    String
  price       Float
  rating      Float
  image       String
  description String?
  createdAt   DateTime @default(now())
}

